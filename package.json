{
  "name": "grapper",
  "version": "1.1.0-beta.12",
  "description": "Data Visualization Microframework",
  "author": "Graphery",
  "homepage": "https://grapper.io",
  "bugs": "https://github.com/graphery/grapper/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphery/grapper.git"
  },
  "main": "dist/component/view.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "workbench:view": "g-workbench -l view -i /src/component/view.js -t test/component/view/cases -p 7000",
    "workbench:shapes": "g-workbench -l view.shapes -i /src/component/view.js -t test/component/shapes/cases -p 7015",
    "workbench:gsvg": "g-workbench -l gSVG -i /src/lib/gsvg.script.js -t test/lib/gsvg/cases -p 7002",
    "workbench:component": "g-workbench -l view -i /src/lib/component.script.js -t test/lib/component/cases -p 7017",
    "workbench:plugin.animateto": "g-workbench -l plugin.animateTo -i /src/lib/gsvg.script.js -i /src/plugins/animateto.script.js -t test/plugins/animateto/cases -p 7003",
    "workbench:plugin.debug": "g-workbench -l plugin.debug -i /src/lib/gsvg.script.js -i /src/plugins/debug.script.js -t test/plugins/debug/cases -p 7004",
    "workbench:plugin.non.scaling.size": "g-workbench -l plugin.keep.aspect -i /src/lib/gsvg.script.js -i /src/plugins/non.scaling.size.script.js -t test/plugins/non.scaling.size/cases -p 7005",
    "workbench:plugin.observe.dark": "g-workbench -l plugin.observe.dark -i /src/component/view.js -i /src/plugins/observe.dark.script.js -t test/plugins/observe.dark/cases -p 7016",
    "workbench:plugin.observe.resize": "g-workbench -l plugin.observe.resize -i /src/lib/gsvg.script.js -i /src/plugins/observe.resize.script.js -t test/plugins/observe.resize/cases -p 7006",
    "workbench:plugin.observe.style": "g-workbench -l plugin.observe.style -i /src/lib/gsvg.script.js -i /src/plugins/observe.style.script.js -t test/plugins/observe.style/cases -p 7007",
    "workbench:plugin.shapes": "g-workbench -l plugin.shapes -i /src/lib/gsvg.script.js -i /src/plugins/shapes.script.js -t test/plugins/shapes/cases -p 7009",
    "workbench:plugin.shapes.extra": "g-workbench -l plugin.shapes.extra -i /src/lib/gsvg.script.js -i /src/plugins/shapes.extra.script.js -t test/plugins/shapes.extra/cases -p 7016",
    "workbench:plugin.template.engine": "g-workbench -l plugin.template.engine -i /src/lib/gsvg.script.js -i /src/plugins/template.engine.script.js -t test/plugins/template.engine/cases -p 7008",
    "workbench:plugin.intersection": "g-workbench -l plugin.intersection -i /src/lib/gsvg.script.js -i /src/plugins/template.engine.script.js -i /src/plugins/intersection.script.js -t test/plugins/intersection/cases -p 7012",
    "workbench:plugin.object": "g-workbench -l plugin.object -i /src/lib/gsvg.script.js -i /src/plugins/template.engine.script.js -i /src/plugins/object.script.js -t test/plugins/object/cases -p 7013",
    "workbench:plugin.tosource": "g-workbench -l toSource -i /src/lib/gsvg.script.js -i /src/plugins/template.engine.script.js -i /src/plugins/tosource.script.js -t test/plugins/tosource/cases -p 7014",
    "workbench:plugin.template.profile": "g-workbench -l plugin.template.profile -i /src/lib/gsvg.script.js -i /src/plugins/template.engine.script.js -i /src/plugins/template.profile.script.js -t test/plugins/template.profile/cases -p 7015",
    "test": "concurrently -m 1 npm:test:*",
    "test:view": "playwright test test/component/view/index.test.js --project=view",
    "test:shapes": "playwright test test/component/shapes/index.test.js --project=view-shapes",
    "test:gsvg": "playwright test test/lib/gsvg/index.test.js --project=svg",
    "test:component": "playwright test test/lib/component/index.test.js --project=component",
    "test:plugin.animateto": "playwright test test/plugins/animateto/index.test.js --project=animateto",
    "test:plugin.debug": "playwright test test/plugins/debug/index.test.js --project=debug",
    "test:plugin.non.scaling.size": "playwright test test/plugins/non.scaling.size/index.test.js --project=non.scaling.size",
    "test:plugin.observe.dark": "playwright test test/plugins/observe.dark/index.test.js --project=observe.dark",
    "test:plugin.observe.resize": "playwright test test/plugins/observe.resize/index.test.js --project=observe.resize",
    "test:plugin.observe.style": "playwright test test/plugins/observe.style/index.test.js --project=observe.style",
    "test:plugin.shapes": "playwright test test/plugins/shapes/index.test.js --project=shapes",
    "test:plugin.shapes.extra": "playwright test test/plugins/shapes.extra/index.test.js --project=shapes.extra",
    "test:plugin.template.engine": "playwright test test/plugins/template.engine/index.test.js --project=template.engine",
    "test:plugin.intersection": "playwright test test/plugins.intersection/index.test.js --project=plugin.intersection",
    "test:plugin.object": "playwright test test/plugins/object/index.test.js --project=object",
    "test:plugin.tosource": "playwright test test/plugins/tosource/index.test.js --project=tosource",
    "test:plugin.template.profile": "playwright test test/plugins/template.profile/index.test.js --project=template.profile",
    "build": "concurrently -m 1 npm:build:*",
    "build:view": "g-packer -l view src/component/view.js ./dist/view.js ./package.json",
    "build:gsvg": "g-packer -l gsvg ./src/lib/gsvg.script.js ./dist/lib/gsvg.js ./package.json",
    "build:component": "g-packer -l component ./src/lib/component.script.js ./dist/lib/component.js ./package.json",
    "build:plugin.animateto": "g-packer -l plugin.animateto src/plugins/animateto.script.js ./dist/plugins/animateto.js",
    "build:plugin.debug": "g-packer -l plugin.debug src/plugins/debug.script.js ./dist/plugins/debug.js",
    "build:plugin.non.scaling.size": "g-packer -l plugin.non.scaling.size src/plugins/non.scaling.size.script.js ./dist/plugins/non.scaling.size.js",
    "build:plugin.observe.resize": "g-packer -l plugin.observe.resize src/plugins/observe.resize.script.js ./dist/plugins/observe.resize.js",
    "build:plugin.observe.style": "g-packer -l plugin.observe.style src/plugins/observe.style.script.js ./dist/plugins/observe.style.js",
    "build:plugin.shapes": "g-packer -l plugin.shapes src/plugins/shapes.script.js ./dist/plugins/shapes.js",
    "build:plugin.shapes.extra": "g-packer -l plugin.shapes.extra src/plugins/shapes.extra.script.js ./dist/plugins/shapes.extra.js",
    "build:plugin.template.engine": "g-packer -l plugin.template.engine src/plugins/template.engine.script.js ./dist/plugins/template.engine.js",
    "build:plugin.intersection": "g-packer -l plugin.intersection src/plugins/intersection.script.js ./dist/plugins/intersection.js",
    "build:plugin.object": "g-packer -l plugin.object src/plugins/object.script.js ./dist/plugins/object.js",
    "build:plugin.tosource": "g-packer -l plugin.tosource src/plugins/tosource.script.js ./dist/plugins/tosource.js",
    "build:plugin.template.profile": "g-packer -l plugin.template.profile src/plugins/template.profile.script.js ./dist/plugins/template.profile.js",
    "build:module:view": "g-packer -l module:view -m src/component/view.js ./dist/module/view.js ./package.json",
    "build:module:gsvg": "g-packer -l module:gsvg -m ./src/lib/gsvg.js ./dist/module/lib/gsvg.js ./package.json",
    "build:module:component": "g-packer -l module:component -m ./src/lib/component.js ./dist/module/lib/component.js ./package.json",
    "build:module:plugin.animateto": "g-packer -l module:plugin.animateto -m src/plugins/animateto.js ./dist/module/plugins/animateto.js",
    "build:module:plugin.debug": "g-packer -l module:plugin.debug -m src/plugins/debug.js ./dist/module/plugins/debug.js",
    "build:module:plugin.non.scaling.size": "g-packer -l module:plugin.non.scaling.size -m src/plugins/non.scaling.size.js ./dist/module/plugins/non.scaling.size.js",
    "build:module:plugin.observe.resize": "g-packer -l module:plugin.observe.resize -m src/plugins/observe.resize.js ./dist/module/plugins/observe.resize.js",
    "build:module:plugin.observe.style": "g-packer -l module:plugin.observe.style -m src/plugins/observe.style.js ./dist/module/plugins/observe.style.js",
    "build:module:plugin.template.engine": "g-packer -l module:plugin.template.engine -m src/plugins/template.engine.js ./dist/module/plugins/template.engine.js",
    "build:module:plugin.shapes": "g-packer -l module:plugin.shapes -m src/plugins/shapes.js ./dist/module/plugins/shapes.js",
    "build:module:plugin.shapes.extra": "g-packer -l module:plugin.shapes.extra -m src/plugins/shapes.extra.js ./dist/module/plugins/shapes.extra.js",
    "build:module:plugin.intersection": "g-packer -l module:plugin.intersection -m src/plugins/intersection.js ./dist/module/plugins/intersection.js",
    "build:module:plugin.object": "g-packer -l module:plugin.object -m src/plugins/object.js ./dist/module/plugins/object.js",
    "build:module:plugin.tosource": "g-packer -l module:plugin.tosource -m src/plugins/tosource.js ./dist/module/plugins/tosource.js",
    "build:module:plugin.template.profile": "g-packer -l module:plugin.template.profile -m src/plugins/template.profile.js ./dist/module/plugins/template.profile.js",
    "update:playwright": "npx playwright install",
    "publish" : "node ./tools/npm-helpers/publish.js"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "concurrently": "^9.2.1",
    "playwright": "^1.55.0",
    "grapper-workbench": "^0.0.4",
    "grapper-packer": "^0.0.3",
    "grapper-core": "^1.0.0-beta.8"
  },
  "keywords": [
    "dataviz",
    "data-visualization",
    "graphs",
    "visualization",
    "data",
    "graph-library",
    "dataviz-library",
    "web-component",
    "svg",
    "graphery"
  ]
}
